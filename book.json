{
    "plugins": [
        "expandable-chapters",
        "page-toc",
        "layout",
        "page-footer"
    ],
    "pluginsConfig": {
        "page-toc": {
            "selector": ".markdown-section h1,.markdown-section h2, .markdown-section h3, .markdown-section h4",
            "position": "before-first"
        }
        "page-footer": {
            "description": "modified at",
            "signature": "Aleen",
            "wisdom": "More than a coder, more than a designer",
            "format": "yyyy-MM-dd hh:mm:ss",
            "copyright": "Copyright &#169; aleen42",
            "timeColor": "#666",
            "copyrightColor": "#666",
            "utcOffset": "8",
            "isShowQRCode": true,
            "baseUri": "https://aleen42.gitbooks.io/personalwiki/content/",
            "isShowIssues": true,
            "repo": "aleen42/PersonalWiki",
            "token": "",
            "style": "normal"
        }  
    }
}